# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SocialLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SocialLogin

# Manage Google+ Login
pod 'GoogleSignIn'

# Manage Facebook Login
pod 'FacebookCore' # '~>0.3.0'
pod 'FacebookLogin' # '~>0.3.0'

# Manage Twitter Login
pod 'TwitterKit'
pod 'SwiftyJSON', '~> 4.0' # Get Data to Dic. for get login user info

# Manage WebService Called | https://github.com/Alamofire/Alamofire.git
pod 'Alamofire', '~> 4.6.0'

end
